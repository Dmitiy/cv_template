include ../mixins/mixin

.skills
    +header-title('skills', 'Skills')
    ul.skills-level
        li
            p Creative
            .skills-level__dots
                - for (var x = 0; x < 9; x++)
                    span.dot.active
                - for (var x = 0; x < 3; x++)
                    span.dot
        li
            p Teamwork
            .skills-level__dots
                - for (var x = 0; x < 11; x++)
                    span.dot.active
                - for (var x = 0; x < 1; x++)
                    span.dot
        li
            p Innovate
            .skills-level__dots
                - for (var x = 0; x < 6; x++)
                    span.dot.active
                - for (var x = 0; x < 6; x++)
                    span.dot
        li
            p Communication
            .skills-level__dots
                - for (var x = 0; x < 11; x++)
                    span.dot.active
                - for (var x = 0; x < 1; x++)
                    span.dot